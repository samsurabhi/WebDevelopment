<% include ../partials/header.ejs %>
<section class="container width40">
	<h3>Contact Us</h3>
	<form method="POST">
		<div class="form-group">
			<label>Name</label>
			<input class="form-control" type="text" name="username" placeholder="Please enter your full name">
		</div>
		<div class="form-group">	
			<label class="form-group">Contact Number</label>
			<input class="form-control" type="text" name="phone" placeholder="Contact number">
		</div>
		<div class="form-group">
			<label class="form-group">Name</label>
			<input class="form-control" type="email" name="email" placeholder="Email">
		</div>	
		<div class="form-group">	
			<label>Subject</label>
			<input class="form-control" type="text" name="subject" placeholder="Topic">
		</div>
		<div class="form-group">
			<label>Discription</label>
			<textarea class="form-control" name="question" width=100% > </textarea>
		</div>
		<input class="btn btn-primary btn-sm btn-block" type="button" value="Submit"><br>
		<input class="btn btn-info btn-sm btn-block" type="reset" value="Reset">
	</form>
</section>


