
<% include templates/header %>

<div class= container>
	<div class="jumbotron">
		<h1>Edit Category:<strong> <%= category.cateName %></strong></h1>
	</div>
<div>

<class = "container">
<form action="/index/<%=category._id%>?_method=PUT" method="POST">
	<div class="form-group">
		<label>Name</label>
		<input class="form-control" disable type="text" name="category[cateName]" value=<%= category.cateName %>>
	</div>
	<div class="form-group">
		<label>Picture Link</label>
		<input class="form-control" type="text" name="category[sampleImg]" value=<%=category.sampleImg%>>
	</div>
	<div class="form-group">
		<label>Brief Discription</label>
		<textarea class="form-control" name="category[discription]"><%=category.discription%></textarea>
	</div>
	<button class="mybtn" type="Submit">Submit</button>
	<button class="mybtn" type="Reset">Reset</button>
	<a class="align-bottom " href="/index/<%=category._id%>">
		<button class="mybtn" >Back</button>
	</a>
</form>	
	